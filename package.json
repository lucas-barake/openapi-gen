{
  "name": "@lucas-barake/openapi-gen",
  "version": "0.4.1",
  "description": "Generate Effect http clients from OpenAPI specs",
  "type": "module",
  "packageManager": "pnpm@9.10.0",
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "scripts": {
    "version": "changeset version",
    "release": "pnpm run build && changeset publish",
    "test": "vitest --run",
    "test:watch": "vitest",
    "coverage": "vitest --run --coverage",
    "clean": "rm -rf dist/*",
    "build": "tsup && pnpm copy-package-json",
    "build:ts": "tsup",
    "copy-package-json": "tsx scripts/copy-package-json.ts",
    "check": "tsgo --noEmit",
    "lint": "oxlint && dprint check",
    "lint-fix": "oxlint --fix && dprint fmt",
    "check:all": "pnpm check && pnpm lint && pnpm test"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "dprint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lucas-barake/openapi-gen.git"
  },
  "keywords": [],
  "author": "Lucas Barake",
  "license": "MIT",
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.4",
    "@effect/cli": "^0.73.2",
    "@effect/experimental": "^0.58.0",
    "@effect/platform": "^0.94.4",
    "@effect/platform-node": "^0.104.1",
    "@effect/vitest": "^0.27.0",
    "@types/node": "^22.15.18",
    "@types/swagger2openapi": "^7.0.4",
    "@typescript/native-preview": "7.0.0-dev.20260209.1",
    "@vitest/coverage-v8": "^3.2.4",
    "dprint": "^0.51.1",
    "effect": "^3.19.16",
    "esbuild": "^0.27.3",
    "oxlint": "^1.42.0",
    "swagger2openapi": "^7.0.8",
    "tsup": "^8.4.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "yaml": "^2.8.0"
  }
}
